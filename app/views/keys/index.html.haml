.page-header
  %h1 Keyring of #{link_to @list, @list}

= render 'lists/list_menu'

.col.col-lg-6.col-md-8
  %table.table
    %thead
      %tr
        %th Fingerprint
        %th Primary UID
    %tbody
      - @keys.each do |key|
        %tr 
          %td= link_to key.fingerprint, list_key_path(@list, key)
          %td= key.email
