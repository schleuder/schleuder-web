- @title = "Keys"

.col-lg-7
  .panel.panel-info
    .panel-heading List's key
    .panel-body
      %table.table
        %thead
          %tr
            %th
            %th
        %tbody
          = render "key_row", key: @list.key

.col-lg-7
  .panel.panel-info
    .panel-heading Subscription keys
    .panel-body
      %table.table
        %thead
          %tr
            %th
            %th= t(".used_by")
        %tbody
          - @assigned_keys.each do |key|
            = render "key_row", key: key

.col-lg-7
  .panel.panel-info
    .panel-heading Other keys
    .panel-body
      %table.table
        %thead
          %tr
            %th
            %th
        %tbody
          - @unassigned_keys.each do |key|
            = render "key_row", key: key

- if current_account.subscribed_to?(@list) || current_account.superadmin?
  .col-lg-6.col-md-8#upload
    .panel.panel-info
      .panel-heading= t(".upload_keys")
      .panel-body
        = render 'keys/form'
